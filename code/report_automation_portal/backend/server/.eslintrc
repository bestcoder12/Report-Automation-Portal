{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["jest", "prettier"],
  "env": {
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "prettier/prettier": ["error", { "singleQuote": true }],
    "import/extensions": [
      "error",
      {
          "js": "ignorePackages",
          "mjs": "ignorePackages"
      }
    ]
  }
}
